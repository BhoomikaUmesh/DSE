import numpy as np
a=np.array([1,2,3,4,5])
b=np.array([6,7,8,9,10])
print("array a: ",a)
print("array b: ",b)
print("sum: ",np.add(a,b))
print("difference: ",np.subtract(a,b))
print("product: ",np.multiply(a,b))
print("division: ",np.divide(a,b))
print("sqrt of array a: ",np.sqrt(a))
print("exponential of array a: ",np.exp(a))
print("min value of a: ",np.min(a))
print("max value of a: ",np.max(a))
print("mean of array a: ",np.mean(a))
print("sd of array a: ",np.std(a))
print("sum of all elements in array a: ",np.sum(a))

c=np.array([[1,2],[3,4],[5,6]])
print("array c: ")
print(c)
print("reshaped array c(2 rows, 3 cols): ")
print(np.reshape(c,(2,3)))

d=np.array([[1,2,3],[4,5,6]])
print("array d: ")
print(d)
print("transposed array d: ")
print(np.transpose(d))
